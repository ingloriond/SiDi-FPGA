        <dip name="Demo Sounds" bits="20" ids="On,Off"/>
        <dip name="Level Select" bits="21" ids="On,Off"/>
        <dip name="Bonus Life" bits="22,23" ids="10k,None,10k/30k,10k/50k/50k"/>
